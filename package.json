{
  "name": "mayra",
  "version": "1.0.0",
  "description": "This repository is just to exemplify the use of unit tests",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent --noStackTrace --runInBand",
    "test:watch": "jest --silent --noStackTrace --runInBand -- --watchAll",
    "test:cls": "jest --clear-cache",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "dev": "tsnd --respawn --transpile-only --cls ./src"
  },
  "keywords": [],
  "author": "Bryan Feiten",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
